       == '\FF\FE\49\6C\4E\59\61\00\62\00');
    assert(utf8toucs2('\EF\BB\BF\E6\B1\89\E5\A5\8E\61\62', 3)
       == '\FE\FF\6C\49\59\4E\00\61\00\62');

